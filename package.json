{
  "name": "color-factory",
  "version": "1.3.1",
  "description": "Color scheme Consideration",
  "author": "techa",
  "license": "MIT",
  "homepage": "https://techa.github.io/color-factory",
  "repository": "techa/color-factory",
  "bug": {
    "url": "https://github.com/techa/color-factory/issues"
  },
  "keywords": [
    "Color Card",
    "Color scheme"
  ],
  "main": "src/index.js",
  "scripts": {
    "start": "npm run browse | npm run build -- -w",
    "clean": "rm -rf dist/*",
    "browse": "browser-sync start --server --index \"index.html\" --files \"src/**/.css, dist/bundle.js\"",
    "build": "rollup -c"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "rollup": "^0.56.4",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-svelte": "^4.1.0",
    "rollup-plugin-uglify": "^3.0.0",
    "svelte": "^2.4.4"
  },
  "dependencies": {
    "base64-js": "^1.3.0",
    "color": "^3.0.0",
    "lzma": "^2.3.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7"
            ]
          },
          "modules": false
        }
      ]
    ]
  }
}
