{
  "name": "color-factory",
  "version": "0.8.0",
  "description": "Color scheme Consideration",
  "author": "techa",
  "license": "MIT",
  "homepage": "https://techa.github.io/color-factory",
  "repository": "techa/color-factory",
  "bug": {
    "url": "https://github.com/techa/color-factory/issues"
  },
  "keywords": [
    "Color Card",
    "Color scheme"
  ],
  "main": "src/index.js",
  "scripts": {
    "start": "npm-run-all -p watch browse",
    "clean": "rm -rf dist/*",
    "watch": "onchange \"src/**/*.{js,html}\" -d 100 -- npm run build:dev",
    "browse": "browser-sync start --server --index \"index.html\" --files \"src/**/.css, dist/bundle.js\" --no-notify",
    "build": "npm-run-all build:*",
    "build:dev": "rollup -c",
    "build:prod": "set NODE_ENV=production && rollup -c ./rollup.config.js"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.1",
    "onchange": "^3.0.2",
    "rollup": "^0.50.0",
    "rollup-plugin-buble": "^0.16.0",
    "rollup-plugin-commonjs": "^8.2.4",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^3.3.0",
    "rollup-plugin-uglify": "^2.0.1",
    "uglify-save-license": "^0.4.1"
  },
  "dependencies": {
    "diff-match-patch": "^1.0.0",
    "tinycolor2": "^1.4.1"
  }
}
