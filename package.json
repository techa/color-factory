{
  "name": "color-factory",
  "version": "0.5.0",
  "description": "Color scheme Consideration",
  "author": "techa",
  "license": "MIT",
  "homepage": "https://techa.github.io/color-factory",
  "repository": "techa/color-factory",
  "bug": {
    "url": "https://github.com/techa/color-factory/issues"
  },
  "keywords": [
    "Color Card",
    "Color scheme"
  ],
  "main": "src/index.js",
  "scripts": {
    "start": "npm-run-all -p watch browse",
    "clean": "rm -rf dist/*",
    "watch": "onchange \"src/**/*.{js,tag}\" -d 100 -- npm run build:dev",
    "browse": "browser-sync start --server --index \"index.html\" --files \"src/**/.css, dist/bundle.js\" --no-notify",
    "build": "npm-run-all build:*",
    "build:dev": "rollup -c",
    "build:prod": "set NODE_ENV=production && rollup -c ./rollup.config.js",
    "build:svelte": "set NODE_ENV=svelte && rollup -c ./rollup.config.js",
    "svelte:watch": "onchange \"src/**/*.{js,html}\" -d 100 -- npm run build:svelte",
    "svelte:browse": "browser-sync start --server --index \"svelte.html\" --files \"src/**/.css, dist/bundle.svelte.js\" --no-notify --port 4000",
    "svelte": "npm-run-all -p svelte:*"
  },
  "devDependencies": {
    "npm-run-all": "^3.1.1",
    "onchange": "^3.0.2",
    "rollup": "^0.36.3",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-riot": "^1.1.0",
    "rollup-plugin-svelte": "^1.6.0",
    "rollup-plugin-uglify": "^1.0.1",
    "uglify-save-license": "^0.4.1"
  },
  "dependencies": {
    "diff-match-patch": "^1.0.0",
    "riot": "^3.3.1",
    "tinycolor2": "^1.4.1"
  }
}
